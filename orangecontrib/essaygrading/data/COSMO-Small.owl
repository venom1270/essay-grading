        <rdfs:subClassOf rdf:resource="#AstronomicalBody"/>
        <rdfs:subClassOf rdf:resource="#Planet-Generic"/>
        <rdfs:subClassOf rdf:resource="#ConvexTangibleObject"/>
    </owl:Class>

