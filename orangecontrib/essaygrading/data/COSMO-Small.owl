              <owl:ObjectProperty rdf:about="#hasShape"/>
            </owl:onProperty>
            <owl:hasValue rdf:resource="#Powdered"/>
          </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#hasColor"/>
            </owl:onProperty>
            <owl:hasValue rdf:resource="#Opaque"/>
