        <hasColor rdf:resource="#Opaque"/>
       <rdfs:subClassOf>
         <owl:Restriction>
           <owl:onProperty>
             <owl:ObjectProperty rdf:about="#hasColor"/>
           </owl:onProperty>
           <owl:hasValue rdf:resource="#Opaque"/>
