  <owl:Class rdf:ID="KosherFood">
        <rdfs:comment>The collection of all food that may be eaten
            according to the #$KosherCodeOfConduct.  This is a
            substance, and food objectgs that are
            Kosher will be catgegorized as KosherFoodObjects.</rdfs:comment>
        <guid>3f98c38c-567b-41d7-8b0b-ded33b166df3</guid>
        <rdfs:subClassOf rdf:resource="#Cuisine"/>
        <rdfs:subClassOf rdf:resource="#CulturalThing"/>
    </owl:Class>
    
  <owl:Class rdf:ID="KosherFoodObject">
        <rdfs:comment>The collection of all organic food objects 
           that may be eaten according to the #$KosherCodeOfConduct.  This is a
            substance, and food objects that are
            Kosher are categorized as KosherFoodObjects.</rdfs:comment>
        <guid>3f98c38c-567b-41d7-8b0b-ded33b166df3</guid>
        <rdfs:subClassOf rdf:resource="#CulturalThing"/>
       <rdfs:subClassOf>
         <owl:Restriction>
