        <guid>209bf3e4-74bf-11d6-8000-00a0c99cc5ae</guid>
        <rdfs:subClassOf rdf:resource="#MacroscopicObject"/>
        <rdfs:subClassOf rdf:resource="#ObjectWithAttribute"/>
       <rdfs:subClassOf>
         <owl:Restriction>
