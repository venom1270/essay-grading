        <rdf:type rdf:resource="#BiologicalOrganType"/>
    </owl:Class>

<owl:Class rdf:ID="Heart">
        <rdfs:comment>The collection of all hearts.  A #$Heart is an
            #$Organ that pumps blood throughout the body of an #$Animal.
            
            SUMO: The Organ that pumps Blood throughout the body.</rdfs:comment>
