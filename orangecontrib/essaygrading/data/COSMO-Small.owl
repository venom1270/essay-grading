 <owl:DatatypeProperty rdf:ID="hasNumericDatatype">
    <rdfs:comment>hasNumericDatatype points from a Number represented as an instance
    of a String in this ontology 
    to a built-in datatype in the implementing system.  This is a crude way of working
    around the limitations of built-in datatypes, which typically cannot be subclassed.</rdfs:comment>
    <rdfs:domain rdf:resource="#NumberString"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

 <owl:DatatypeProperty rdf:ID="hasIntegerDatatype">
    <rdfs:comment>hasNumericDatatype points from an integer represented as an instance
    of a Type in this ontology to a built-in datatype in the implementing system.  This is a crude way of working
    around the limitations of built-in datatypes, which typically cannot be subclassed.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="#hasNumericDatatype"/>
    <rdfs:domain rdf:resource="#IntegerString"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
