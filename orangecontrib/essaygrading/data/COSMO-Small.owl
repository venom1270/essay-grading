-->
    <owl:Class rdf:ID="Cardinality">
      <rdfs:subClassOf rdf:resource="#NonnegativeInteger"/>
      <rdf:type rdf:resource="#QualitativeAttributeType"/>
      <rdfs:comment>A property of a Group or Set: 
        A negative or nonnegative whole number.</rdfs:comment>
    </owl:Class>

<!-- *db603 changed at 603 -->
    <owl:Class rdf:ID="Dimensionality">
      <rdfs:subClassOf rdf:resource="#AttributeType"/>
      <rdf:type rdf:resource="#QuantitativeAttributeType"/>
      <rdfs:comment>A property type of an Object, Region, or Space.
       NOTE: some objects such as abstract or mental objects may
       not have a meaningful dimensionality.</rdfs:comment>
    </owl:Class>

<!-- *db603
      <rdfs:subClassOf rdf:resource="#NonnegativeInteger"/>
-->
  <owl:Class rdf:ID="DimensionalityValue">
      <rdfs:subClassOf rdf:resource="#QuantitativeAttributeValue"/>
      <rdf:type rdf:resource="#QuantitativeAttributeType"/>
      <rdfs:comment>A property of an Object, Region, or Space.
        It consists of a nonnegative integer, but is represented
        as that integer with the 'UnitOfMeasure' being 'Dimensional'.
          Hence:
            (Every Point hasAttributeValue {0 Dimensional})
       </rdfs:comment>
      <rdfs:subClassOf>
